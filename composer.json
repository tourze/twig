{
  "name": "tourze/twig",
  "description": "集成Twig的一些助手方法",
  "minimum-stability": "stable",
  "license": "proprietary",
  "authors": [
    {
      "name": "tourze",
      "email": "dev@tourze.com"
    }
  ],
  "require": {
    "tourze/base": "dev-master",
    "twig/twig": "^1.21",
    "twig/extensions": "^1.3",
    "doctrine/cache": "^1.4",
    "asm89/twig-cache-extension": "^1.0",
    "league/commonmark": "~0.5",
    "aptoma/twig-markdown": "~1.1",
    "njh/twig-html-helpers": "dev-master"
  },
  "autoload": {
    "psr-4": {
      "tourze\\Twig\\": "src/"
    },
    "files": [
      "bootstrap.php"
    ]
  },
  "autoload-dev": {
    "psr-4": {
      "tourze\\Twig\\": "tests/"
    },
    "files": [
      "bootstrap-dev.php"
